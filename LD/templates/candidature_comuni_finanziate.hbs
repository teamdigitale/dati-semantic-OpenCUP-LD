
{
    "@context": {
    "CLV": "https://w3id.org/italia/onto/CLV/",
    "COV": "https://w3id.org/italia/onto/COV/",
    "PRJ": "https://w3id.org/italia/onto/Project/"
    },
    "@graph": [
        {{#each .candidature}}
        {
        "@type": "PRJ:PublicInvestmentProject",
        "@id": "https://w3id.org/italia/data/Project/PublicInvestmentProject/uniqueProjectCodeValue/{{encodeURIComponent codice_cup}}",

        "PRJ:uniqueProjectCodeValue": "{{codice_cup}}",
        "PRJ:projectTitle": "Progetto {{codice_cup}} per il comune {{codice_ipa}} relativo all'avviso {{avviso}}",
        "PRJ:projectTotalCost": {{importo_finanziamento}},
        "PRJ:hasCall": "https://w3id.org/italia/data/Project/Call/{{encodeURIComponent avviso}}"
        }
        {{#unless @last}},{{/unless}}
        {{/each}}
    ]
}